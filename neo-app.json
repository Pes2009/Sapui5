{
  "authenticationMethod": "saml",
  "securityConstraints": [
  	{
  		"permission": "AccessApplicationDescriptor",
  		"description": "Access application description",
  		"protectedPaths": ["/neo-app.json"]
  	}],
  "welcomeFile": "/webapp/index.html",
  "routes": [
    {
      "path": "/resources",
      "target": {
        "type": "service",
        "name": "sapui5",
        "entryPath": "/resources"
      },
      "description": "SAPUI5 Resources"
    },
    
    {
      "path": "/Northwind.svc",
      "target": {
        "type": "destination",
        "name": "northwind_http"
      },
      "description": "HTTP Destination to Northwind"
    }
  ],
  "sendWelcomeFileRedirect": true
}